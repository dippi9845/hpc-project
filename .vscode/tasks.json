{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc-9 compila il file attivo",
            "command": "/usr/bin/gcc-9",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Attivit√† generata dal debugger."
        },
        {
            "label": "make-simd",
            "type": "shell",
            "command": "make simd",
        },
        {
            "label": "make-omp-simd",
            "type": "shell",
            "command": "make omp-simd",
        },
        {
            "label": "make-cuda",
            "type": "shell",
            "command": "make cuda-dbg",
        },
        {
            "label": "make-sph-quad-three",
            "type": "shell",
            "command": "make sph-quad-three",
        },
        {
            "label": "make-sph-gui-quad",
            "type": "shell",
            "command": "make sph-gui-quad",
        }
        
    ],
    "version": "2.0.0"
}